model Section{
    id Int @id @default(autoincrement())
    name String @unique
    src String @unique
    pages PageSections[] 
}

model Page {
    id Int @id @default(autoincrement())
    name String @unique
    sections PageSections[] 
}

model PageSections{
    pageId Int
    sectionId Int
    page Page @relation(fields: [pageId], references: [id])
    section Section @relation(fields: [sectionId], references: [id])
    data Json @default("{}")
    @@id([pageId, sectionId])
}