model Section{
    id Int @id @default(autoincrement())
    name String @unique
    componentId String @unique
    pages PageSections[] 
}

model Page {
    id Int @id @default(autoincrement())
    name String @unique
    sections PageSections[] 
}

model PageSections{
    id Int @id @default(autoincrement()) 
    pageId Int
    sectionId Int
    page Page @relation(fields: [pageId], references: [id])
    section Section @relation(fields: [sectionId], references: [id])
    data Json @default("{}")
   
}